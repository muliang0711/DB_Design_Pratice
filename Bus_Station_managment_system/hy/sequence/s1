CREATE SEQUENCE TICKET_ID_SEQ START WITH 1 NOCACHE NOCYCLE;
DROP SEQUENCE TICKET_ID_SEQ;



CREATE SEQUENCE SEQ_TICKET_ID
  START WITH 2
  INCREMENT BY 1
  MINVALUE 1
  MAXVALUE 999999999999999999999  -- 可以调大，避免溢出
  NOCYCLE
  NOCACHE;


CREATE SEQUENCE SEQ_PAYMENT_ID
  START WITH 1
  INCREMENT BY 1
  MINVALUE 1
  MAXVALUE 999999999
  NOCYCLE
  NOCACHE;


CREATE SEQUENCE SEQ_POINT_TXN_ID
  START WITH 1
  INCREMENT BY 1
  MINVALUE 1
  MAXVALUE 999999999
  NOCYCLE
  NOCACHE;

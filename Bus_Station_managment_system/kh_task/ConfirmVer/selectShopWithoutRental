COLUMN shopId        FORMAT A10  HEADING 'Shop ID'
COLUMN shopNo        FORMAT A8   HEADING 'Shop No'
COLUMN shopTenantName FORMAT A25 HEADING 'Tenant Name'
COLUMN stationId     FORMAT A10  HEADING 'Station'
COLUMN shopType      FORMAT A12  HEADING 'Shop Type'
COLUMN status        FORMAT A10  HEADING 'Shop Status'

SELECT 
    s.shopId,s.shopNo,s.shopTenantName,s.stationId,s.shopType,s.status
FROM shop s
LEFT JOIN rentalCollection rc 
ON s.shopId = rc.shopId
WHERE rc.shopId IS NULL
ORDER BY s.stationId, s.shopNo;
